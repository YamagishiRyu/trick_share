    = link_to 'Google', 'http://www.google.com'
    = link_to 'TrickShare', '/'
    %nav
      %ul.nav.navbar-nav.navbar-right
        %li
          = link_to 'Yahoo!', 'http://www.yahoo.com'
        - if logged_in?
          %li.dropdown
            %a.dropdown-toggle{:href => '#', :data => {:toggle => 'dropdown'}} 
              アカウント 
              %b.caret 　
            %ul.dropdown-menu
              %li 
                = link_to 'プロフィール', current_contributor
